{
  "name": "react-client",
  "version": "0.1.0",
  "license": "ISC",
  "author": "Milad Irannejad",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./config/jest/setup.js"
    ]
  },
  "scripts": {
    "lint": "eslint config node src webpack.*.js",
    "lint:watch": "esw --watch config node src webpack.*.js",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:update": "npm run test -- --updateSnapshot",
    "dev:api": "cd node/api && json-server --config config.json data.json",
    "dev:webpack": "webpack-dev-server --config webpack.dev.js",
    "dev:node": "babel-node node/dev.js",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:node": "babel-node node/build.js",
    "prod:node": "babel-node node/prod.js"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "express-winston": "^2.5.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bulma": "^0.7.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
