    "mock:nats": "node test/mock/nats/index.js"
  },
  "dependencies": {
    "@grpc/proto-loader": "0.5.3",
    "axios": "0.19.2",
    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "express-winston": "4.0.3",
    "google-protobuf": "3.11.4",
    "graphiql": "0.17.5",
    "graphql": "14.6.0",
    "graphql-playground-middleware-express": "1.7.12",
    "graphql-tools": "4.0.7",
    "grpc": "1.24.2",
    "jaeger-client": "3.17.2",
    "lodash": "4.17.15",
    "nats": "1.4.0",
    "opentracing": "0.14.4",
    "prom-client": "12.0.0",
    "uuid": "7.0.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "chalk": "3.0.0",
    "json-server": "0.16.1",
    "mocha": "7.0.1",
    "nock": "12.0.1",
    "nyc": "15.0.0",
