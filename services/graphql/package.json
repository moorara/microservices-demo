  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "start": "node src/server.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive src",
    "test:component": "NODE_ENV=test mocha test/component/index.js",
