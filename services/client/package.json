    "enzyme-to-json": "3.4.3",
    "eslint": "6.5.1",
    "eslint-watch": "6.0.1",
    "font-awesome": "4.7.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react-test-renderer": "16.11.0",
    "redux": "4.0.4",
