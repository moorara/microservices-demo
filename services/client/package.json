    "opn": "6.0.0",
    "request": "2.88.0"
  },
  "scripts": {
    "lint": "eslint src server mock",
    "lint:fix": "eslint --fix src server mock",
    "lint:watch": "esw --watch src server mock",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:update": "react-scripts test --updateSnapshot",
